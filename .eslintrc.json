{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "indent": [2, 4],
        "react/jsx-indent": [2, 4],
        "react/jsx-indent-props": "off",
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }]
    }
}